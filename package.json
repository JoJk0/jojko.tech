{
  "packageManager": "pnpm@8.6.8",
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "start": "node .output/server/index.mjs",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "up": "taze major -I",
    "postinstall": "nuxi prepare",
    "generate": "nuxi generate",
    "functions:deploy": "cd ./functions && firebase deploy --only functions",
    "start:generate": "npx serve .output/public",
    "functions:dev": "cd ./functions && npm run build && firebase emulators:start --only functions"
  },
  "dependencies": {
    "gsap": "^3.12.2",
    "jjk-ui": "../jjk-ui",
    "swiper": "^10.0.4",
    "three": "^0.154.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@iconify-json/carbon": "^1.1.18",
    "@iconify-json/twemoji": "^1.1.11",
    "@nuxt/devtools": "^0.6.7",
    "@nuxtjs/color-mode": "^3.3.0",
    "@pinia/nuxt": "^0.4.11",
    "@types/three": "^0.154.0",
    "@vite-pwa/nuxt": "^0.1.0",
    "@vueuse/nuxt": "^10.2.1",
    "consola": "^3.2.3",
    "eslint": "^8.45.0",
    "firebase-admin": "^11.10.1",
    "firebase-functions": "^4.4.1",
    "firebase-functions-test": "^3.1.0",
    "firebase-tools": "^12.4.4",
    "nuxt": "^3.6.3",
    "pinia": "^2.1.4",
    "sass": "^1.63.6",
    "taze": "^0.11.2",
    "typescript": "^5.1.6",
    "vue-tsc": "^1.8.5"
  }
