{
  "type": "module",
  "packageManager": "pnpm@8.6.7",
  "scripts": {
    "build": "vite-ssg build",
    "dev": "vite --port 2137",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
