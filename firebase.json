{
  "database": {
    "rules": "database.rules.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "functions": {
    "source": ".output/server"
  },
  "hosting": [
    {
      "site": "jjk-nuxt-example",
      "public": ".output/public",
      "cleanUrls": true,
      "rewrites": [
        {
          "source": "**",
          "function": "server"
        }
      ]
    }
  ],
  "emulators": {
    "functions": {
      "port": 2139
    },
    "database": {
      "port": 2141
    },
    "hosting": {
      "port": 2138
    },
    "storage": {
      "port": 2140
    },
    "ui": {
      "enabled": true
    }
  }
}
